<launch>
    <node pkg="e2es" type="keyboard_ctl" name="keyboard_ctl" output="screen">
        <param name="max_z" type="double" value="2.5" />
        <param name="min_z" type="double" value="-0.1" />
        <param name="max_x" type="double" value="20" />
        <param name="min_x" type="double" value="-20" />
        <param name="max_y" type="double" value="20" />
        <param name="min_y" type="double" value="-20" />
        <param name="velocity_horizon" type="double" value="1.0" />
        <param name="velocity_vertical" type="double" value="0.5" />
        <param name="angular_velocity_turn" type="double" value="0.3" />
    </node>
</launch>
